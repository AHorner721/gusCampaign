<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- ios support -->
    <link rel="apple-touch-icon" href="/img/icons/icon96x96.png" />
    <meta name="apple-mobile-web-app-status-bar" content="#ffffff" />
    <meta name="theme-color" content="#0d47a1" />
    <!--Import Google Icon Font-->
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <!-- google fonts -->
    <link
      href="https://fonts.googleapis.com/css2?family=Playfair+Display:ital,wght@0,400;0,500;0,600;0,700;1,800&display=swap"
      rel="stylesheet"
    />
    <!-- Compiled and minified CSS -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css"
    />
    <link rel="stylesheet" href="/css/main.css" />
    <link rel="manifest" href="../manifest.json" />
    <title>Alderman Black | Photos</title>
  </head>
  <body>
    <nav class="nav-wrapper white">
      <div class="container">
        <a href="/" id="app-title-bar" class="brand-logo indigo-text darken-4">
          Alderman Gus Black
        </a>
      </div>
    </nav>

    <h2 class="tagline blue-text text-darken-4 center" style="margin-top: 70px">
      <i class="starIcons material-icons">star</i>
      Photo Gallery
      <i class="starIcons material-icons">star</i>
    </h2>
    <!-- first row -->
    <article class="row center">
      <!-- images -->
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/monument.jpg"
          alt="visiting monument in DC"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/olyminpic-meeting.jpg"
          alt="meeting olympic athelete"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/girlswhocode.jpg"
          alt="girls coding camp"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/governor-meeting.jpg"
          alt="Gus meeting governor"
        />
      </div>
    </article>
    <article class="row center">
      <!-- images -->
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/army-corp-eng.jpg"
          alt="Gus with Army Corp Engineer colleague"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/district-ceremony.jpg"
          alt="Gus attending Richland Exchange Club District Ceremony"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/work-pic.jpg"
          alt="Gus with Army Corp of Engineer Director Dr. Pittman and company"
        />
      </div>
      <div class="col s12 l3 imgRow">
        <img
          class="responsive-img materialboxed"
          src="/img/blackfam-v2.jpg"
          alt="Family photo of Gus with Oretha, Garsheo Jr. and Ashly in Marine Corp dress blues."
        />
      </div>
      <div id="blue" class="center">
        <a href="/" class="waves-effect waves-blue btn-flat">
          <i class="material-icons left">arrow_back</i>Return</a
        >
      </div>
    </article>
    <!-- min jquery lib -->
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <!-- Compiled and minified Materialize Lib -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script>
      $(document).ready(function () {
        $(".modal").modal();
        $(".materialboxed").materialbox();
      });
    </script>
  </body>
</html>
